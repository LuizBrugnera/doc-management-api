    "/upload/file/{userId}": {
      "post": {
        "summary": "Upload de Documento",
        "description": "Realiza o upload de um documento para o usuário especificado.",
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
